{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ranmsDF"
		},
		"ranmsBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ranmsBlobStorage'"
		},
		"ranmsEDWConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ranmsEDWConnection'"
		},
		"ranmsSQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ranmsSQLDB'"
		},
		"ranmsKVConnection_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://ranmsKV.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ranmsBlobStorage_connectionString": {
						"value": "[parameters('ranmsBlobStorage_connectionString')]"
					},
					"ranmsEDWConnection_connectionString": {
						"value": "[parameters('ranmsEDWConnection_connectionString')]"
					},
					"ranmsSQLDB_connectionString": {
						"value": "[parameters('ranmsSQLDB_connectionString')]"
					},
					"ranmsKVConnection_properties_typeProperties_baseUrl": {
						"value": "[parameters('ranmsKVConnection_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}